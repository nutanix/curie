[global]
bs=1M
iodepth=64
rw=write
ioengine=libaio
direct=1
disk_util=0
continue_on_error=all
group_reporting

; 512G file is split into 16 of 32G regions.
; 2G is read in every region
; WSS = 2G x 16 = 32G
[db-dss1]
filesize=512G
size=32G
zonesize=2G
zoneskip=30G
filename=/dev/sdd

[db-dss2]
filesize=512G
size=32G
zonesize=2G
zoneskip=30G
filename=/dev/sde

[db-dss3]
filesize=512G
size=32G
zonesize=2G
zoneskip=30G
filename=/dev/sdf

[db-dss4]
filesize=512G
size=32G
zonesize=2G
zoneskip=30G
filename=/dev/sdg

[db-log1]
size=2G
filename=/dev/sdb

[db-log2]
size=2G
filename=/dev/sdc
